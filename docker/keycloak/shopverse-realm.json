{
  "realm": "shopverse",
  "enabled": true,
  "sslRequired": "none",
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "roles": {
    "realm": [
      {
        "name": "default-roles-shopverse",
        "description": "Default roles for realm",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "offline_access",
        "composite": false,
        "clientRole": false
      },
      {
        "name": "uma_authorization",
        "composite": false,
        "clientRole": false
      }
    ]
  },
  "clients": [
    {
      "clientId": "shopverse-api",
      "name": "ShopVerse API",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "shopverse-api-secret",
      "redirectUris": ["*"],
      "webOrigins": ["*"],
      "bearerOnly": false,
      "consentRequired": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "publicClient": false,
      "frontchannelLogout": false,
      "protocol": "openid-connect",
      "protocolMappers": [
        {
          "name": "audience-shopverse-api",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-audience-mapper",
          "consentRequired": false,
          "config": {
            "included.client.audience": "shopverse-api",
            "id.token.claim": "false",
            "access.token.claim": "true"
          }
        }
      ]
    }
  ],
  "users": [
    {
      "username": "testuser",
      "enabled": true,
      "email": "testuser@yopmail.com",
      "emailVerified": true,
      "firstName": "Test",
      "lastName": "User",
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": ["default-roles-shopverse", "offline_access", "uma_authorization"]
    }
  ]
}
